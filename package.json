{
  "name": "roverscontroller",
  "version": "0.0.0",
  "description": "This algorithm should guide robotic rovers in mars through giving a grid and x y coordinates to moviment then.(This is repository is part of company test).",
  "main": "src/main.js",
  "scripts": {
    "start": "node ./src/main.js",
    "test": "jest --coverage --runInBand --env=node --detectOpenHandles",
    "test:coverage": "npm test -- --coverage",
    "test:badges": "npm run test:coverage  && jest-coverage-badges"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ran-j/roversController.git"
  },
  "jest": {
    "verbose": true,
    "testEnvironment": "node",
    "coverageReporters": [
      "json-summary",
      "text",
      "lcov"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 20,
        "functions": 20,
        "lines": 20
      }
    }
  },
  "keywords": [
    "robots",
    "mars"
  ],
  "author": "Ran-j",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ran-j/roversController/issues"
  },
  "homepage": "https://github.com/ran-j/roversController#readme",
  "dependencies": {
    "jest": "^26.6.3",
    "jest-coverage-badges": "^1.1.2",
    "jsdoc": "^3.6.6",
    "mock-stdin": "^1.0.0"
  }
}
